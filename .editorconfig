root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = false
max_line_length = 120
indent_size = 4
ij_continuation_indent_size = 4
ij_smart_tabs = false
ij_visual_guides = none
ij_wrap_on_typing = false

